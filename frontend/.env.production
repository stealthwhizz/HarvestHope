# Production environment variables for Harvest Hope
# These will be set in AWS Amplify console

# API Gateway URL - will be replaced during deployment
VITE_API_BASE_URL=https://api.harvest-hope.com

# CloudFront distribution URL for assets
VITE_ASSETS_BASE_URL=https://assets.harvest-hope.com

# Environment
VITE_NODE_ENV=production

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Game configuration
VITE_GAME_VERSION=1.0.0
VITE_MAX_SAVE_SLOTS=5
VITE_AUTO_SAVE_INTERVAL=30000

# AWS Region
VITE_AWS_REGION=us-east-1