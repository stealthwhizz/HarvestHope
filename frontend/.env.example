# Harvest Hope Environment Configuration Template
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:3000

# Weather API (set this to your deployed Lambda API Gateway URL)
# VITE_WEATHER_API_URL=https://your-api-gateway-url.amazonaws.com/weather

# Google Gemini AI API (get free key from: https://aistudio.google.com/app/apikey)
# IMPORTANT: Replace 'your-gemini-api-key-here' with your actual API key
VITE_GEMINI_API_KEY=your-gemini-api-key-here

# Example (after you get your key):
# VITE_GEMINI_API_KEY=AIzaSyABC123def456GHI789jkl012MNO345pqr

# Development Configuration
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DEBUG_LOGGING=true